{
  "expo": {
    "name": "Single Screen Starter",
    "description": "simple react native single screen application starter with expo",
    "scheme": "exposinglescreenstarter",
    "slug": "exposinglescreenstarter",
    "privacy": "public",
    "version": "0.0.2",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/calebnance/expo-single-screen-starter",
    "orientation": "portrait",
    "primaryColor": "#1d2d3a",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#1d2d3a",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["src/assets/**/*"],
    "android": {
      "versionCode": 2
    },
    "ios": {
      "buildNumber": "0.0.2",
      "supportsTablet": true
    }
  }
}
